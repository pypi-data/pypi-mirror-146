{
    "localhost": {
        "aedir_db_params": {
            "accesslog": {
                "directory": "accesslog",
                "mdb_checkpoint": "5000 2",
                "mdb_envflags": "writemap mapasync",
                "mdb_maxsize": 5000000000,
                "mdb_mode": "0600",
                "mdb_rtxnsize": 10000,
                "mdb_searchstack": 16,
                "sizelimit": -1,
                "suffix": "cn=accesslog-ae-dir",
                "syncprov_checkpoint": "5000 2",
                "syncprov_sessionlog": 10000,
                "timelimit": 120
            },
            "um": {
                "directory": "um",
                "mdb_checkpoint": "1000 1",
                "mdb_envflags": "writemap mapasync",
                "mdb_maxsize": 50000000,
                "mdb_mode": "0600",
                "mdb_rtxnsize": 10000,
                "mdb_searchstack": 16,
                "sizelimit": 500,
                "suffix": "ou=ae-dir",
                "syncprov_checkpoint": "1000 1",
                "syncprov_sessionlog": 10000,
                "timelimit": 60
            },
            "session": {
                "directory": "session",
                "mdb_checkpoint": "1000 1",
                "mdb_envflags": "writemap mapasync",
                "mdb_maxsize": 50000000,
                "mdb_mode": "0600",
                "mdb_rtxnsize": 10000,
                "mdb_searchstack": 16,
                "sizelimit": 500,
                "suffix": "ou=ae-session",
                "syncprov_checkpoint": "1000 1",
                "syncprov_sessionlog": 10000,
                "timelimit": 60
            }
        },
        "aedir_hosts": {
            "provider": ["localhost"],
            "consumer": ["localhost"]
        },
        "openldap_slapo_allowed": false,
        "openldap_rootdse_alt_servers": ["localhost"],
        "openldap_schema_files": [
            "ae-dir.schema",
            "ae-dir-dcr.schema",
            "ae-dir-session.schema",
            "draft-findlay-ldap-groupofentries.schema",
            "draft-stroeder-mailboxrelatedobject.schema",
            "draft-stroeder-namedobject.schema",
            "mspwdreset.schema",
            "oath-ldap.schema",
            "openssh-lpk_openldap.schema",
            "stroeder.com-oid-macros.schema",
            "stroeder.com.schema",
            "sudoers.schema",
            "autofs.schema",
            "mail.schema"
        ],
        "aedir_accesslog_suffix": "cn=accesslog-ae-dir",
        "aedir_addressbook_attrs": ["cn", "sn", "givenName", "mail", "userCertificate"],
        "aedir_aeauthctoken_serial_regex": "[a-z0-9_-]+",
        "aedir_aedept_deptnumber_regex": "[a-zA-Z0-9_.:/-]+",
        "aedir_aegroup_cn_regex": "[a-z0-9]+-[a-z0-9-]+",
        "aedir_aegroup_member_filter": "(&(|(objectClass=aeUser)(objectClass=aeService))(aeStatus=0))",
        "aedir_aemailgroup_member_filter": "(&(|(objectClass=inetLocalMailRecipient)(objectClass=aeContact))(mail=*)(aeStatus=0))",
        "aedir_aelocation_cn_regex": "[a-zA-Z0-9_.:/-]+",
        "aedir_aenwdevice_visibility_sets": ["this/-2 & user/-1", "this/-2 & user/-1/aeRequires"],
        "aedir_aeperson_uniqueid_regex": "[a-zA-Z0-9_.:/-]+",
        "aedir_aeservice_sshpubkey_regex": "(ssh-rsa|ssh-dss|ecdsa-sha2-nistp256|ssh-ed25519) [A-Za-z0-9+=/]+ .+",
        "aedir_aeservice_uid_regex": "[a-zA-Z0-9_.-]+",
        "aedir_aesrvgroup_cn_regex": "[a-z0-9]+-[a-z0-9-]+",
        "aedir_aesudorule_cn_regex": "[a-z0-9]+-[a-z0-9-]+",
        "aedir_aetag_cn_regex": "[a-z0-9]+-[a-z0-9-]+",
        "aedir_aeuser_sshpubkey_regex": "(ssh-rsa|ssh-dss|ecdsa-sha2-nistp256|ssh-ed25519) [A-Za-z0-9+=/]+ .+",
        "aedir_aeuser_uid_regex": "[a-z]{4}+",
        "aedir_aeuser_write_pw_restrictions": [],
        "aedir_aezone_cn_regex": "[a-z0-9]+",
        "aedir_confidential_person_attrs" :["employeeNumber", "aeNotBefore", "aeNotAfter"],
        "aedir_fake_search_roots": [],
        "aedir_homedirectory_hidden": "-/-",
        "aedir_homedirectory_prefixes": ["/home"],
        "aedir_login_shells": ["/bin/bash", "/bin/true", "/bin/false"],
        "aedir_main_provider_hostname": "demo.ae-dir.com",
        "aedir_max_gid": 59999,
        "aedir_max_uid": 59999,
        "aedir_min_gid": 30000,
        "aedir_min_uid": 30000,
        "aedir_org_zone": "people",
        "aedir_providers_restrictions": [],
        "aedir_pwsync_dn_regex": "",
        "aedir_srvgroup": "ae-dir-provider-hosts",
        "aedir_pwsync_listener_user": "root",
        "aedir_pwsync_socket_path": "/opt/ae-dir/run/pwsync",
        "aedir_replicas_restrictions": [],
        "aedir_session_suffix": "cn=ae-session",
        "aedir_sshkey_perms":["pty", "X11-forwarding", "agent-forwarding", "port-forwarding", "user-rc"],
        "aedir_sshpublickey_self_filter": "(&(objectClass=aeUser)(oathToken=*))",
        "aedir_suffix": "ou=ae-dir",
        "aedir_unique_person_zones": ["base"],
        "aedir_username_length": 4,
        "aedir_who_srvgroup": "(user/-1 | user/aeSrvGroup | user/-1/aeProxyFor)",
        "aeticketid_regex": "[A-Z]+-[0-9]+",
        "ansible_managed": "AE-DIR automatic tests",
        "getent_passwd": {
            "aehostd": ["aehostd", 369, 369]
        },
        "inventory_hostname": "localhost",
        "oath_ldap_enabled": "False",
        "openldap_argon2_params": "p=1 t=3 m=51200",
        "openldap_cacert_pathname": "tests/tls/ca-chain.pem",
        "openldap_cert_pathname": "tests/tls/localhost.crt",
        "openldap_conn_max_pending": 100,
        "openldap_conn_max_pending_auth": 1000,
        "openldap_slapo_accesslog": false,
        "openldap_slapo_authzid": true,
        "openldap_slapo_noopsrch": true,
        "openldap_dhparam_numbits": 2048,
        "openldap_dhparam_pathname": "tests/tls/slapd-dhparams.pem",
        "openldap_ec_name": "X25519:secp521r1:secp384r1:prime256v1",
        "openldap_idletimeout": "900",
        "openldap_key_pathname": "tests/tls/localhost.key",
        "openldap_lastbind_precision": 1,
        "openldap_listener_threads": 1,
        "openldap_log_level": "stats stats2",
        "openldap_log_purge": "3660+00:00 07+00:00",
        "openldap_noopsrch_enabled": "1",
        "openldap_password_hash": "{ARGON2}",
        "openldap_role": "provider",
        "openldap_service_fqdn": "localhost",
        "openldap_sizelimit": 100,
        "openldap_sockbuf_max_incoming": "131072",
        "openldap_sockbuf_max_incoming_auth": "4194304",
        "openldap_syncrepl_providers": [],
        "openldap_syncrepl_tls_cipher_suite": "ECDHE-RSA-AES256-GCM-SHA384:DHE-RSA-AES256-GCM-SHA384:ECDH-RSA-AES256-GCM-SHA384:!ADH",
        "openldap_syncrepl_tls_protocol_min": "3.3",
        "openldap_threads": 2,
        "openldap_tls_cert_suffixes": [
            "ou=example ou,o=example company"
        ],
        "openldap_tls_cipher_suite": "ECDHE-RSA-AES256-GCM-SHA384:DHE-RSA-AES256-GCM-SHA384:ECDH-RSA-AES256-GCM-SHA384:ECDHE-RSA-AES128-GCM-SHA256:DHE-RSA-AES128-GCM-SHA256:ECDH-RSA-AES128-GCM-SHA256:DHE-RSA-AES256-SHA256:DHE-RSA-AES256-SHA:!ADH",
        "openldap_tls_protocol_min": "3.3"
    }
}
