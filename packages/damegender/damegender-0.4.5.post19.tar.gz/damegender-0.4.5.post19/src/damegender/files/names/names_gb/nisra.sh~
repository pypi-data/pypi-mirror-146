cd orig

# COPY FROM HERE

mkdir -p nisra
cp ../nisra.py nisra
cd nisra
wget -c https://www.nisra.gov.uk/sites/nisra.gov.uk/files/publications/Full_Name_List_9718.xlsx

ssconvert -S Full_Name_List_9718.xlsx Full_Name_List_9718.csv

cp Full_Name_List_9718.csv.4 northireland.females.csv
sed -i '1d' northireland.females.csv
sed -i '1d' northireland.females.csv
cp Full_Name_List_9718.csv.3 northireland.males.csv
sed -i '1d' northireland.males.csv
sed -i '1d' northireland.males.csv
python3 nisra.py
